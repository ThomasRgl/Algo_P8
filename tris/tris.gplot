##
set datafile separator ","

##
set title "nombre de ticks cpu pour trier un vecteur pseudo aleatoire de taille x "
set key autotitle columnhead
set xlabel "taille"
set ylabel "tick cpu"

#
set key left top maxrows 4


##
set terminal "png" size 1500,600
set output "trace1.png" # adapter si besoin

##
set autoscale
set yrange [1:328996]
set logscale x 2
set logscale y 2

#

#

plot x * x t "n^2" with l lw 2 lc rgb "gray0", \
log(x)* x t "nlog(n)" with l lw 2 lc rgb "gray20", \
x t "n" with l lw 2 lc rgb "gray40", \
log(x) t "log(n)" with l lw 2 lc rgb "gray60", \
for [i=2:7] "data/random_data.txt" using 1:i with linespoints linewidth 2



